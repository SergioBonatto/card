"myCard": {
  "info": {
    "name": "Sergio Bonatto",
    "handle": "sergiobonatto",
    "twitter": "fiBonatto",
    "npm": "sergiobonatto",
    "github": "sergiobonatto",
    "linkedin": "sergiobonatto",
    "web": "https://bonatto.vercel.app/",
    "work": "fiBonatto tech",
    "subtitle": "Software Engineer | Programming Language Enthusiast"
  },
  "data": [
    "          <white>{{name}}</> / <cyan>{{handle}}</>",
    "",
    "<cyan>{{subtitle}}</>",
    "",
    {
      "label": "<blue>Work</>",
      "text": "{{work}}"
    },
    "",
    {
      "label": "X",
      "text": "https://x.com/<cyan>{{twitter}}</>",
      "when": "{{twitter}}"
    },
    {
      "label": "npm",
      "text": "https://www.npmjs.com/<red>~{{npm}}</>",
      "when": "{{npm}}"
    },
    {
      "label": "GitHub",
      "text": "https://github.com/<green>{{github}}</>",
      "when": "{{github}}"
    },
    {
      "label": "LinkedIn",
      "text": "https://www.linkedin.com/in/<blue>{{linkedin}}</>",
      "when": "{{linkedin}}"
    },
    {
      "label": "Web",
      "text": "<cyan>{{web}}</>",
      "when": "{{web}}"
    },
    "",
    {
      "label": "Card",
      "text": "<red>npx</> {{_packageName}} ðŸš€"
    }
  ]
},
