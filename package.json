    "prepare": "node src/htmlify",
    "prepublishOnly": "rm -rf dist && webpack",
    "build": "rm -rf dist && webpack",
    "test": "jest"
  },
  "bin": {
    "bonatto": "bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "index.html"
  ],
